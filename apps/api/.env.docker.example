# Docker Compose Environment Variables
# Copy this to .env in the same directory as docker-compose.yml

# PostgreSQL credentials (used for both dev and test containers)
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_secure_password

# Development database
POSTGRES_DB_DEV=reservation_knex_dev

# Test database
POSTGRES_DB_TEST=reservation_knex_test

# pgAdmin (development only)
PGADMIN_EMAIL=your_email@example.com
PGADMIN_PASSWORD=your_pgadmin_password
